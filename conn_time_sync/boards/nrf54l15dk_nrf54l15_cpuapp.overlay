&pinctrl {
	i2s20_default: i2s20_default {
		group1 {
			psels = <NRF_PSEL(I2S_LRCK_M, 1, 11)>,
				<NRF_PSEL(I2S_SCK_M, 1, 12)>,
				<NRF_PSEL(I2S_SDIN, 1, 13)>;
		};
	};
};

&i2s20 {
	status = "okay";
	pinctrl-0 = <&i2s20_default>;
	pinctrl-names = "default";
};

&uart20 {
	current-speed = <1000000>;
};
